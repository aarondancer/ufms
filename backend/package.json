{
  "name": "aaron-dancer-d424-capstone",
  "version": "1.0.0",
  "description": "D424 Capstone",
  "author": "Aaron Dancer <adancer@wgu.edu>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:prod": "tsc -p tsconfig.build.json",
    "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
    "start:watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -T src/main.ts",
    "start:dev": "ts-node -T src/main.ts",
    "start:prod": "node dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:seed": "ts-node -T ./src/database/seed.ts",
    "migration:run": "yarn run typeorm migration:run -d ./src/database/ormconfig.ts",
    "migration:show": "yarn run typeorm migration:show -d ./src/database/ormconfig.ts",
    "migration:generate": "node ./src/database/generate.js",
    "migration:create": "node ./src/database/create.js",
    "migration:revert": "yarn run typeorm migration:revert",
    "schema:drop": "yarn run typeorm schema:drop -d ./src/database/ormconfig.ts",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "webpack": "webpack --config webpack.config.js --progress",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "apos-to-lex-form": "^1.0.5",
    "bcrypt": "^5.1.0",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-ctx": "^0.1.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "natural": "^6.8.0",
    "nodemon": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "spelling-corrector": "^3.0.0",
    "stopword": "^2.0.8",
    "swagger-ui-express": "^5.0.0",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.17",
    "typeorm-transactional": "0.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.1.8",
    "@nestjs/testing": "^10.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/passport-jwt": "^3.0.9",
    "@types/stopword": "^2.0.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "jest-clean-console-reporter": "^0.3.0",
    "jest-html-reporters": "^3.1.4",
    "lint-staged": "15.1.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "diagnostics": false
        }
      ]
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-html-reporters"
    ]
  }
}
